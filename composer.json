{
    "name": "gbprod/elastica-extra-bundle",
    "description": "Bundle providing extra elastica tools for managing indices and types settings",
    "type": "bundle",
    "require": {
        "php": ">=5.6",
        "ruflin/elastica": "^3.1|^5.0|^6.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/yaml": "^2.7|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "license": "WTFPL",
    "authors": [
        {
            "name": "GBProd",
            "email": "contact@gb-prod.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GBProd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\GBProd\\": "tests/"
        }
    }
}
